{
  "name": "personal-website",
  "version": "1.0.0",
  "description": "my resume website",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run copy && npm run compilecss && npm run transpile",
    "clean": "rimraf dist",
    "compilecss": "node-sass --source-map true src/css/main.scss dist/css/main.css",
    "compilecss:prod": "node-sass src/css/main.scss dist/css/main.css",
    "copy": "npm run copyhtml && npm run copyvendorjs && npm run copyassets",
    "copyhtml": "copyfiles -f src/views/* dist",
    "copyvendorjs": "copyfiles -f node_modules/jquery/dist/jquery.js node_modules/bootstrap/dist/js/bootstrap.js node_modules/popper.js/dist/popper.js dist/js",
    "copyassets": "copyfiles -u 1 'src/assets/**/*' dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "transpile": "babel src/js --out-dir dist/js --source-maps",
    "transpile:prod": "babel src/js --out-dir dist/js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wflooter480/personal-website.git"
  },
  "author": "Whitney Bowden",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wflooter480/personal-website/issues"
  },
  "homepage": "https://github.com/wflooter480/personal-website#readme",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.0.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "copyfiles": "^1.2.0",
    "node-sass": "^4.7.2",
    "rimraf": "^2.6.2"
  }
}
